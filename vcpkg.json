{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "phoenix-editor",
  "version": "0.2.0",
  "description": "Modern C++20 non-linear video editor (NLE)",
  "homepage": "https://github.com/example/phoenix-editor",
  "license": "MIT",
  "supports": "windows & x64",
  "dependencies": [
    {
      "name": "ffmpeg",
      "features": [
        "all",
        "nvcodec",
        "qsv",
        "gpl",
        "x264",
        "x265",
        "webp",
        "avcodec",
        "avformat",
        "swscale",
        "swresample"
      ]
    },
    "sdl2",
    "spdlog",
    "fmt",
    "nlohmann-json",
    "stb"
  ],
  "overrides": [],
  "builtin-baseline": "984f9232b2fe0eb94f5e9f161d6c632c581fff0c"
}

